<h1>My Bookings</h1>


<% @bookings.each do |booking| %>
  <li>
    <%= booking %>
    <%= booking.office.title %>
    <%= booking.office.user.name %>
    <%= booking.start_date %>
    <%= booking.end_date %>
    <%= link_to 'Update booking', edit_booking_path(booking) %>
    <%= link_to 'Cancel', booking_path(booking), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %>
  </li>
<% end %>
