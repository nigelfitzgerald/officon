<%= simple_form_for :office do |f| %>
  <%= f.input :name %>
  <%= f.input :location %>
  <div class="input-group mb-3">
  <%= f.label :start_date, "Start Date:" %>
  <%= f.date_field :start_date, class: 'form-control'%>
  <%= f.label :end_date, "End Date:" %>
  <%= f.date_field :end_date, class: 'form-control' %>
  </div>
  <%= f.submit %>
<% end %>
